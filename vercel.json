{
    "version": 2,
    "name": "vercel-functions-demo",
    "builds": [
        { "src": "/app/go/*.go", "use": "@vercel/go" },
        { "src": "/app/go/date/*.go", "use": "@vercel/go" },
        { "src": "/app/python/main.py", "use": "@vercel/python" }
    ],
    "routes": [
        { "src": "/go", "dest": "/app/go" },
        { "src": "/go/date", "dest": "/app/go/date" },
        { "src": "/python", "dest": "/app/python"}
    ]
}